{
  "name": "fwa.dev",
  "version": "1.0.8",
  "description": "Source for fwa.dev",
  "keywords": [
    "functional",
    "web",
    "app",
    "dynamic",
    "serverless"
  ],
  "author": "Brian LeRoux <b@brian.io>",
  "scripts": {
    "start": "sandbox",
    "lint": "arc-hydrate && eslint src --fix"
  },
  "architect": {
    "app": "fwa",
    "http": {
      "/blog/*": {
        "src": "src/blog",
        "method": "any"
      },
      "/*": {
        "src": "src/default",
        "method": "any"
      }
    }
  },
  "dependencies": {
    "@architect/eslint-config": "^1.0.0",
    "@architect/functions": "RC",
    "@architect/hydrate": "^1.10.2",
    "@architect/sandbox": "^3.7.4",
    "eslint": "^7.31.0"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config",
    "rules": {
      "require-await": "off"
    }
  }
}
